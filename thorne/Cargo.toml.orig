[package]
name = "thorne"
version = "0.1.7"
authors = ["Cis Osil <ofekbendavid9@gmail.com>"]
edition = "2018"
license = "MIT"
description = "A random string generator and random word generator library"
readme = "README.MD"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[dependencies]
rand = "0.8.3"

[lib]
name = "thorne"
path = "lib/lib.rs"

[[bin]]
name = "thorne"
path = "lib/main.rs"